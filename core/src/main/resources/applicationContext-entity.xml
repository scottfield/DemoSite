<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <!-- This file will contain bean overrides for extensions of Broadleaf entities -->
    <!-- For example, if you have a custom class that extends CustomerImpl, you would note that here -->

    <!--override broadleaf bean definition-->
    <bean id="org.broadleafcommerce.core.catalog.domain.Product"
          class="com.cdfamedy.core.domain.impl.CustomProductImpl"
          scope="prototype"/>
    <bean id="org.broadleafcommerce.profile.core.domain.Address"
          class="com.cdfamedy.core.domain.impl.CustomAddressImpl" scope="prototype"/>
    <bean id="org.broadleafcommerce.profile.core.domain.CustomerAttribute"

          class="org.broadleafcommerce.profile.core.domain.CustomerAttributeImpl"
          scope="prototype"/>
    <bean id="org.broadleafcommerce.profile.core.domain.Customer"
          class="com.cdfamedy.core.domain.impl.CustomCustomerImpl" scope="prototype"/>
    <bean id="org.broadleafcommerce.core.order.domain.Order"
          class="com.cdfamedy.core.domain.impl.CustomOrderImpl" scope="prototype"/>

    <!--customized bean definition-->
    <bean id="com.cdfamedy.core.domain.Shop"
          class="com.cdfamedy.core.domain.impl.ShopImpl"
          scope="prototype"/>
    <bean id="com.cdfamedy.core.domain.QRCode"
          class="com.cdfamedy.core.domain.impl.QRCodeImpl"
          scope="prototype"/>
    <bean id="com.cdfamedy.core.domain.ShopAccount"
          class="com.cdfamedy.core.domain.impl.ShopAccountImpl" scope="prototype"/>
    <bean id="com.cdfamedy.core.domain.FiveCard"
          class="com.cdfamedy.core.domain.impl.FiveCardImpl" scope="prototype"/>
    <bean id="com.cdfamedy.core.domain.CustomerFiveCardXref"
          class="com.cdfamedy.core.domain.impl.CustomerFiveCardXrefImpl" scope="prototype"/>
    <bean id="com.cdfamedy.core.domain.Coupon"
          class="com.cdfamedy.core.domain.impl.CouponImpl" scope="prototype"/>
    <bean id="com.cdfamedy.core.domain.CustomerCouponXref"
          class="com.cdfamedy.core.domain.impl.CustomerCouponXrefImpl" scope="prototype"/>

</beans>